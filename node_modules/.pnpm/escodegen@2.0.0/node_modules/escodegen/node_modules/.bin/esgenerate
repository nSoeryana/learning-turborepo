#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/nanasuryana/Documents/OFFICE/Research/my-turborepo/node_modules/.pnpm/escodegen@2.0.0/node_modules/escodegen/bin/node_modules:/Users/nanasuryana/Documents/OFFICE/Research/my-turborepo/node_modules/.pnpm/escodegen@2.0.0/node_modules/escodegen/node_modules:/Users/nanasuryana/Documents/OFFICE/Research/my-turborepo/node_modules/.pnpm/escodegen@2.0.0/node_modules:/Users/nanasuryana/Documents/OFFICE/Research/my-turborepo/node_modules/.pnpm/node_modules:/Users/nanasuryana/Documents/OFFICE/Research/my-turborepo/node_modules:/Users/nanasuryana/Documents/OFFICE/Research/node_modules:/Users/nanasuryana/Documents/OFFICE/node_modules:/Users/nanasuryana/Documents/node_modules:/Users/nanasuryana/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/nanasuryana/Documents/OFFICE/Research/my-turborepo/node_modules/.pnpm/escodegen@2.0.0/node_modules/escodegen/bin/node_modules:/Users/nanasuryana/Documents/OFFICE/Research/my-turborepo/node_modules/.pnpm/escodegen@2.0.0/node_modules/escodegen/node_modules:/Users/nanasuryana/Documents/OFFICE/Research/my-turborepo/node_modules/.pnpm/escodegen@2.0.0/node_modules:/Users/nanasuryana/Documents/OFFICE/Research/my-turborepo/node_modules/.pnpm/node_modules:/Users/nanasuryana/Documents/OFFICE/Research/my-turborepo/node_modules:/Users/nanasuryana/Documents/OFFICE/Research/node_modules:/Users/nanasuryana/Documents/OFFICE/node_modules:/Users/nanasuryana/Documents/node_modules:/Users/nanasuryana/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/esgenerate.js" "$@"
else
  exec node  "$basedir/../../bin/esgenerate.js" "$@"
fi
