#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/nanasuryana/Documents/OFFICE/Research/my-turborepo/node_modules/.pnpm/strip-indent@1.0.1/node_modules/strip-indent/node_modules:/Users/nanasuryana/Documents/OFFICE/Research/my-turborepo/node_modules/.pnpm/strip-indent@1.0.1/node_modules:/Users/nanasuryana/Documents/OFFICE/Research/my-turborepo/node_modules/.pnpm/node_modules:/Users/nanasuryana/Documents/OFFICE/Research/my-turborepo/node_modules:/Users/nanasuryana/Documents/OFFICE/Research/node_modules:/Users/nanasuryana/Documents/OFFICE/node_modules:/Users/nanasuryana/Documents/node_modules:/Users/nanasuryana/node_modules:/Users/node_modules:/node_modules:/Users/nanasuryana/Documents/OFFICE/Research/my-turborepo/node_modules/.pnpm/redent@1.0.0/node_modules/redent/node_modules:/Users/nanasuryana/Documents/OFFICE/Research/my-turborepo/node_modules/.pnpm/redent@1.0.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/nanasuryana/Documents/OFFICE/Research/my-turborepo/node_modules/.pnpm/strip-indent@1.0.1/node_modules/strip-indent/node_modules:/Users/nanasuryana/Documents/OFFICE/Research/my-turborepo/node_modules/.pnpm/strip-indent@1.0.1/node_modules:/Users/nanasuryana/Documents/OFFICE/Research/my-turborepo/node_modules/.pnpm/node_modules:/Users/nanasuryana/Documents/OFFICE/Research/my-turborepo/node_modules:/Users/nanasuryana/Documents/OFFICE/Research/node_modules:/Users/nanasuryana/Documents/OFFICE/node_modules:/Users/nanasuryana/Documents/node_modules:/Users/nanasuryana/node_modules:/Users/node_modules:/node_modules:/Users/nanasuryana/Documents/OFFICE/Research/my-turborepo/node_modules/.pnpm/redent@1.0.0/node_modules/redent/node_modules:/Users/nanasuryana/Documents/OFFICE/Research/my-turborepo/node_modules/.pnpm/redent@1.0.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../strip-indent@1.0.1/node_modules/strip-indent/cli.js" "$@"
else
  exec node  "$basedir/../../../../../strip-indent@1.0.1/node_modules/strip-indent/cli.js" "$@"
fi
